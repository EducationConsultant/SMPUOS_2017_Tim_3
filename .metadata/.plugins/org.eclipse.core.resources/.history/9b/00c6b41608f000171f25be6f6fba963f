var app = angular.module('bioskopApp.routes', ['ngRoute']);

app.config(['$routeProvider','$httpProvider', function ($routeProvider, $httpProvider) {
    $routeProvider
        .when('/',{
            templateUrl : 'html/pocetna.html'
        })
        
        
        
        
        
        
        
        
        
        
        
        
        
}]);